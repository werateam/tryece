  <script>
    var count= 0;
    function darkMode()
    {
       count = count+1;
      var element = document.getElementsByTagName('html')[0];
      element.style.filter="invert(1)";
      if(count==2){
       count =0;
       element.style.filter="invert(0)";
      }
    }
  </script>


  